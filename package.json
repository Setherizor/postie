    "eris": "^0.15.1",
    "express": "^4.17.1",
    "lowdb": "^2.1.0",
    "request": "^2.88.2",
    "serve-favicon": "^2.5.0",
    "snowflake-util": "^1.0.1",
    "sodium-native": "^3.2.1",
    "supports-color": "^9.0.1",
    "ytdl-core": "^4.8.3",
