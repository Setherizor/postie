{
  "name": "postie-bot",
  "version": "2.7.0",
  "description": "A Discord bot that does some awesome stuff",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
